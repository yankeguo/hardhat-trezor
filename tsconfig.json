{
  "compilerOptions": {
    "composite": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "outDir": "./dist",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2021"
  },
  "exclude": [
    "./dist",
    "./node_modules"
  ]
}